{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.LOLLIPOP_MR1) {
    ColorStateList sl=new ColorStateList(new int[][]{new int[]{-android.R.attr.state_checked},new int[]{android.R.attr.state_checked}},new int[]{DialogUtils.resolveColor(getContext(),android.R.attr.textColorSecondary),color});
    setButtonTintList(sl);
  }
}
