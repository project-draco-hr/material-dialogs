{
  if (ThemeSingleton.get(false) == null)   return;
  ThemeSingleton s=ThemeSingleton.get();
  theme(s.darkTheme ? Theme.DARK : Theme.LIGHT);
  if (s.titleColor != -1)   titleColorRes(s.titleColor);
  if (s.contentColor != -1)   contentColorRes(s.contentColor);
  if (s.accentColor != -1)   accentColorRes(s.accentColor);
  if (s.itemColor != -1)   itemColorRes(s.itemColor);
  if (s.icon != -1)   iconRes(s.icon);
  if (s.backgroundColor != -1)   backgroundColorRes(s.backgroundColor);
  if (s.dividerColor != -1)   dividerColorRes(s.dividerColor);
}
