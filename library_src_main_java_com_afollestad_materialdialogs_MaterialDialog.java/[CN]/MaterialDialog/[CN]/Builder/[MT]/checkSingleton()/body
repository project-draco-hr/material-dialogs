{
  if (ThemeSingleton.get(false) == null)   return;
  ThemeSingleton s=ThemeSingleton.get();
  theme(s.darkTheme ? Theme.DARK : Theme.LIGHT);
  if (s.titleColor != -1)   titleColor(s.titleColor);
  if (s.contentColor != -1)   contentColor(s.contentColor);
  if (s.accentColor != -1)   accentColor(s.accentColor);
  if (s.itemColor != -1)   itemColor(s.itemColor);
  if (s.icon != null)   icon(s.icon);
  if (s.backgroundColor != -1)   backgroundColor(s.backgroundColor);
  if (s.dividerColor != -1)   dividerColor(s.dividerColor);
}
