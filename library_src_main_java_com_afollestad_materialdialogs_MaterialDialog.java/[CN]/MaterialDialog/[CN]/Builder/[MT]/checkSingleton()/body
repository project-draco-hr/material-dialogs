{
  if (ThemeSingleton.get(false) == null)   return;
  ThemeSingleton s=ThemeSingleton.get();
  theme(s.darkTheme ? Theme.DARK : Theme.LIGHT);
  if (s.titleColor != 0)   titleColor(s.titleColor);
  if (s.contentColor != 0)   contentColor(s.contentColor);
  if (s.accentColor != 0)   accentColor(s.accentColor);
  if (s.itemColor != 0)   itemColor(s.itemColor);
  if (s.icon != null)   icon(s.icon);
  if (s.backgroundColor != 0)   backgroundColor(s.backgroundColor);
  if (s.dividerColor != 0)   dividerColor(s.dividerColor);
}
