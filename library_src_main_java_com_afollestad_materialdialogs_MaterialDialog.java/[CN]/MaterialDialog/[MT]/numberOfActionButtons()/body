{
  int number=0;
  if (mBuilder.positiveText != null && positiveButton.getVisibility() == View.VISIBLE)   number++;
  if (mBuilder.neutralText != null && neutralButton.getVisibility() == View.VISIBLE)   number++;
  if (mBuilder.negativeText != null && negativeButton.getVisibility() == View.VISIBLE)   number++;
  return number;
}
