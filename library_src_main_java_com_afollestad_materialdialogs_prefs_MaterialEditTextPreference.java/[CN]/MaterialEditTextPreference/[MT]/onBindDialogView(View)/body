{
  getEditText().setText("");
  if (getText() != null)   getEditText().setText(getText());
  ViewParent oldParent=getEditText().getParent();
  if (oldParent != view) {
    if (oldParent != null)     ((ViewGroup)oldParent).removeView(getEditText());
    onAddEditTextToDialogView(view,getEditText());
  }
}
