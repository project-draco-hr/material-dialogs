{
  new MaterialDialog.Builder(this).title(R.string.socialNetworks).adapter(new ButtonItemAdapter(this,R.array.socialNetworks),new MaterialDialog.ListCallback(){
    @Override public void onSelection(    MaterialDialog dialog,    View itemView,    int which,    CharSequence text){
      showToast("Clicked item " + which);
    }
  }
).show();
}
