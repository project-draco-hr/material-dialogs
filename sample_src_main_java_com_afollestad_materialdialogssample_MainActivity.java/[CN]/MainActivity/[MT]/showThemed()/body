{
  new MaterialDialog.Builder(this).title(R.string.permissions).content(R.string.permissionsContent).positiveText(R.string.accept).negativeText(R.string.decline).positiveColorRes(R.color.material_red_400).titleAlignment(Alignment.CENTER).titleColorRes(R.color.material_red_400).theme(Theme.DARK).build().show();
}
